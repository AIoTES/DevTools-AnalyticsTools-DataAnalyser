            "options": {
                "type": "object",
                "description": "Service options.",
                "properties": {
                    "wordAttr": {
                        "type": "string",
                        "description": "The name of the data attribute that contains the word text."
                    },
                    "countAttr": {
                        "type": "string",
                        "description": "The name of the data attribute that contains the word counts."
                    },
                    "width": {
                        "type": "integer",
                        "description": "The width of the chart in pixels."
                    },
                    "height": {
                        "type": "integer",
                        "description": "The height of the chart in pixels."
                    },
                    "color": {
                        "type": "string",
                        "description": "The color of the words in the wordcloud, as an SVG color name or a #xxxxxx value. It can also start with '$' followed by the name of a categorical attribute to use for coloring the words according to it."
                    }
                },
                "required": ["wordAttr", "countAttr", "width", "height", "color"]
            }
        },
        "required": ["data", "options"]
    },
